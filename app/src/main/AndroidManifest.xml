<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.biznnovate.entrenaste_demo" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".user_profile"
            android:label="@string/title_activity_user_profile" >
        </activity>
        <activity
            android:name=".gym_profile"
            android:label="@string/title_activity_gym_profile" >
        </activity>
        <activity
            android:name=".gyms_by_category"
            android:label="@string/title_activity_gyms_by_category"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".gym_classes"
            android:label="@string/title_activity_gym_classes" >
        </activity>
        <activity
            android:name=".functional_gym"
            android:label="@string/title_activity_functional_gym"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.biznnovate.entrenaste_demo.MainActivity" />
        </activity>
        <activity
            android:name=".gyms_by_cat_crossfit"
            android:label="@string/title_activity_gyms_by_cat_crossfit"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.biznnovate.entrenaste_demo.MainActivity" />
        </activity>
    </application>

</manifest>
