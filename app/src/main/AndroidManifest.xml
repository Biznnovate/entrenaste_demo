        </activity>
        <activity
            android:name=".gym_classes_crossfit_pty"
            android:label="@string/title_activity_gym_classes_crossfit_pty"
            android:parentActivityName=".profile_crossfit_pty" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.biznnovate.entrenaste_demo.profile_crossfit_pty" />
